<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>

  <p>
    <strong>Category</strong>
    <%= post.category %>
  </p>


  <p>
    <strong>User id:</strong>
    <%= post.user_id %>
  </p>

  <p>
    <%= image_tag post.image %>
  </p>

</div>
